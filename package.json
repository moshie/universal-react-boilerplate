{
  "name": "universal-react-boilerplate",
  "version": "0.0.0",
  "description": "Simple boilerplate designed for universal React applications.",
  "author": "David Hewitt <hello@wp-unite.io>",
  "scripts": {
    "start": "webpack && node ./src/server.js",
    "dev": "npm-run-all --parallel watch:*",
    "watch:webpack": "webpack -w",
    "watch:server": "nodemon ./src/server.js --exec babel-node --ext js,jsx --ignore public/"
  },
  "main": "server.js",
  "keywords": [
    "universal",
    "isomorphic",
    "react",
    "tutorial"
  ],
  "dependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "express": "4.13.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "4.4.6",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "redux": "^3.7.2",
    "webpack": "^3.3.0"
  },
  "devDependencies": {
    "nodemon": "1.11.0",
    "npm-run-all": "3.1.2"
  },
  "license": "MIT"
}
